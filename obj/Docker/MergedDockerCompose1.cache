services:
  apigateway:
    build:
      context: D:\Codes\eRecruitmentOnMicroservices
      dockerfile: ApiGateway/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: ApiGateway
      target: base
    container_name: ApiGateway
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages;/root/.nuget/fallbackpackages2
      Urls__applying: http://applying.api
      Urls__identity: http://identity.api
      Urls__jobs: http://jobs.api
    image: apigateway:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages --additionalProbingPath
        /root/.nuget/fallbackpackages2  "/app/bin/Debug/netcoreapp3.1/ApiGateway.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 4005
      target: 80
    - published: 5005
      target: 443
    tty: true
    volumes:
    - D:\Codes\eRecruitmentOnMicroservices\ApiGateway:/app:rw
    - C:\Users\AhmedAH\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\AhmedAH\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\AhmedAH\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Microsoft\Xamarin\NuGet:/root/.nuget/fallbackpackages:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages2:ro
    - C:\Users\AhmedAH\.nuget\packages:/root/.nuget/packages:ro
    - D:\Codes\eRecruitmentOnMicroservices:/src:rw
